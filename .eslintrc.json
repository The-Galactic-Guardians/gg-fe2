{
	"extends": [
		"airbnb",
		"airbnb-typescript",
		"plugin:prettier/recommended",
		"prettier"
	],
	"parserOptions": {
		"project": "./tsconfig.json"
	},
	"rules":{
		"no-console": [
			"error",
			{
				"allow": [
					"info",
					"error",
					"warn"
				]
			}
		] 
	}
}


// allow console.info console.error and console.warn - error for console.log